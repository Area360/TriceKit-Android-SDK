<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nz.co.area360.tricekit.maps.samples">

    <application
        android:name="nz.co.area360.tricekit.maps.samples.MainApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- TriceKit Credentials -->
        <!--<meta-data android:name="trice_username" android:value="TRICEKIT_USERNAME" />-->
        <!--<meta-data android:name="trice_api_key" android:value="TRICEKIT_API_KEY" />-->

        <activity
            android:name="nz.co.area360.tricekit.maps.samples.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="nz.co.area360.tricekit.maps.samples.MapActivity" android:theme="@style/TriceMapTheme" />
        <activity android:name="nz.co.area360.tricekit.maps.samples.MapFragmentActivity" android:theme="@style/TriceMapTheme" />

    </application>

</manifest>
